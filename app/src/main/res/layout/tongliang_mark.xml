<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/numTxt"
        android:background="@drawable/login_edit_shape"
        android:paddingLeft="3dip"
        android:paddingRight="3dip"
        android:layout_toRightOf="@+id/roundIcon"
        android:gravity="center"
        style="@style/txt_black_12"
        android:text="11.11"
        android:layout_width="wrap_content"
        android:layout_height="30dip" />
    <View
        android:layout_below="@+id/numTxt"
        android:id="@+id/roundIcon"
        android:background="@drawable/shape_round_tongliangyujin3"
        android:layout_width="10dip"
        android:layout_height="10dip"/>
</RelativeLayout>
